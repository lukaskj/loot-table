{
   "name": "@lukaskj/item-drop-table",
   "version": "2.0.0",
   "license": "MIT",
   "main": "dist/index.js",
   "types": "dist/index.d.ts",
   "scripts": {
      "prepublish": "yarn build",
      "prepare": "husky install",
      "clean": "rimraf dist coverage",
      "start": "ts-node src/index.ts",
      "build": "yarn lint && yarn clean && tsc -p tsconfig.build.json",
      "test": "npx jest --runInBand --config jest.config.js",
      "test:watch": "npx jest --runInBand --config jest.config.js --watch",
      "test:cov": "npx jest --runInBand --config jest.config.js --coverage",
      "lint": "yarn eslint . --ext .ts,.tsx",
      "lint:fix": "yarn eslint . --ext .ts,.tsx --fix",
      "pre-commit": "yarn lint:fix",
      "pre-push": "yarn test"
   },
   "files": [
      "dist/**/*"
   ],
   "exclude": [
      "node_modules",
      "dist",
      "test"
   ],
   "devDependencies": {
      "@faker-js/faker": "^7.1.0",
      "@types/jest": "^27.5.1",
      "@types/seedrandom": "^3.0.2",
      "@types/uuid": "^8.3.4",
      "@typescript-eslint/eslint-plugin": "^5.26.0",
      "@typescript-eslint/parser": "^5.26.0",
      "eslint": "^8.16.0",
      "eslint-config-prettier": "^8.5.0",
      "eslint-plugin-import": "^2.26.0",
      "eslint-plugin-prettier": "^4.0.0",
      "husky": "^8.0.1",
      "jest": "^28.1.0",
      "prettier": "^2.6.2",
      "rimraf": "^3.0.2",
      "ts-jest": "^28.0.3",
      "ts-loader": "^9.3.0",
      "ts-node": "^10.8.0",
      "typescript": "^4.7.2"
   },
   "dependencies": {
      "class-transformer": "^0.5.1",
      "class-validator": "^0.13.2",
      "reflect-metadata": "^0.1.13",
      "seedrandom": "^3.0.5"
   }
}
